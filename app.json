{
  "expo": {
    "name": "HypeConnect",
    "slug": "hype-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "hypeconnect",
    "main": "expo-router/entry",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.hypeconnect",
      "newArchEnabled": false
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourcompany.hypeconnect",
      "newArchEnabled": false
    },
    "web": {
      "output": "static"
    }
  }
}